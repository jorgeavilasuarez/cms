/**
* Dibuja los selects de edades
*/
{template .dialog}
<div class="{css dialogo}" style="display: none;">
    <div class="{css ui-widget-overlay}" style="display: block;">
        <div class="css ui-dialog;" style="display: block;">
        </div>
    </div>
    <img id="img_cortinilla">
</div>
{/template}